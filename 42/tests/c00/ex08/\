// ecrire une fonction qui affiche les diff combinaisons possibles de n chifres >>>

#include <unistd.h>

void	ft_print_combn(int n);
void	ft_putchar(char a);


void	ft_print_combn(int n)
{
	char *str;
	int	i;
	int	j;

	i = 0;

	while (i <= n)
	{
		str[i] = '0';
		i += 1;
	}
	j = 0;
	while (i != 0)
	{
		while (j <= 9)
		{
			ft_putchar(str[j] = str[j] + 1);
			j++;
		}
	}
}
int main()
{
	ft_print_combn(1);
	return 0;
}
